[uwsgi]
uid = www-data
gid = www-data
pythonpath = /path/to/ce1sus
plugins = python27
module = ce1sus-run
workers = 2
vacuum = True
max-requests = 1000000
harakiri = 600
harakiri-verbose = true
need-app = true
buffer-size = 32768
listen = 128
die-on-term = true

#master-as-root = true
max-fd = 10240
#memory-report = true
#stats = /tmp/stats.socket
chown-socket = www-data